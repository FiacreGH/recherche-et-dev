{
	"name": "typo3cms/bootstrap-introduction",
	"description": "Bootstrap introduction package for TYPO3 CMS",
	"keywords": ["bootstrap", "introduction-package", "typo3", "cms"],
	"homepage": "http://bootstrap.ecodev.ch",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Fabien Udriot",
			"email": "fabien.udriot@ecodev.ch",
			"homepage": "http://ecodev.ch"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "*",
		"typo3cms/vhs": "*",
		"typo3cms/flux": "*",
		"typo3cms/fluidpages": "*",
		"typo3cms/fluidcontent": "*",
		"typo3cms/fluidcontent_bootstrap": "*",
		"typo3cms/introduction": "*",
		"typo3cms/realurl": "*",
		"typo3cms/news": "*"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "typo3cms/flux",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/NamelessCoder/flux",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/vhs",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/NamelessCoder/vhs",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidpages",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/NamelessCoder/fluidpages",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidcontent",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/NamelessCoder/fluidcontent",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/fluidcontent_bootstrap",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/NamelessCoder/fluidcontent_bootstrap",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/introduction",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "https://github.com/Ecodev/introduction",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/realurl",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "git://git.typo3.org/TYPO3v4/Extensions/realurl.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "typo3cms/news",
				"version": "master",
				"type": "typo3-flow-package",
				"source": {
					"url": "git://git.typo3.org/TYPO3v4/Extensions/news.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	],
	"extra": {
		"installer-paths": {
			"htdocs/typo3conf/ext/{$name}/": [
				"typo3cms/flux",
				"typo3cms/vhs",
				"typo3cms/fluidpages",
				"typo3cms/fluidcontent",
				"typo3cms/fluidcontent_bootstrap",
				"typo3cms/introduction",
				"typo3cms/realurl",
				"typo3cms/news"
			]
		}
	},
	"minimum-stability": "dev",
	"require-dev": {

	}
}
